<article>
    <img src="/images/<%= product.image %>" alt="<%= product.name %>" width="100%">
    <div>
        <i class="fas fa-truck"></i>
        <p class="precio">$<%= product.price %></p>
        <p class="descuento"> <%= product.descuento %>% off </p>
        <h4 class="descripcion">
            <%= product.name %>
        </h4>
    </div>
</article>